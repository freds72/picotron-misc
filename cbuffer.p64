--[[pod_format="raw",created="2024-04-13 06:23:24",modified="2024-04-13 07:01:46",revision=21]]
function add_span(root,x0,x1)
 if(not root) return {x0=x0,x1=x1}
 -- left
 -- xxxxx
 --       ****
 if x1<root.x0 then
 	root.left=add_span(root.left,x0,x1)
 	return
 end
 -- right
 --       xxxx
 --  ****
 if x0>root.x1 then
 	root.right=add_span(root.right,x0,x1)
 	return
 end
 
 -- straddling right
 --    xxxxxxx
 --  *******
 if x1>root.x1 then
  root.right=add_span(root.right,root.x1+1,x1)
  return
 end
 -- straddling left
 -- xxxxxxx
 --     *******
 if x0<root.x0 then
  root.left=add_span(root.left,x0,root.x0-1)
  return
 end
  
 -- hidding
 --    xx
 --  *******
 return root 
end

function print_spans(root)
 if(not root) return
 local s=""
 for i=1,root.x0-1 do
  s..=" "
 end
 for i=root.x0,root.x1 do
  s..="x"
 end
 print(s)
 print_spans(root.left)
 print_spans(root.right)
end

local root=add_span(nil,5,12)
add_span(root,5,18)
add_span(root,6,9)
add_span(root,1,29)
print_spans(root)


